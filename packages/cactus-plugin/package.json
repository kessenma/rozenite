{
  "name": "cactus-rozenite",
  "description": "Cactus LLM inspection for Rozenite.",
  "version": "1.0.0-alpha.29",
  "type": "module",
  "main": "./dist/react-native.cjs",
  "module": "./dist/react-native.js",
  "types": "./dist/react-native.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json",
    "logo.png"
  ],
  "scripts": {
    "build": "npx rozenite build",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "version:prerelease": "npm version prerelease --no-git-tag-version",
    "predeploy": "npm run build",
    "deploy:npm": "npm run predeploy && npm run version:prerelease && npm publish --tag alpha",
    "deploy:patch": "npm run predeploy && npm run version:patch && npm publish",
    "deploy:minor": "npm run predeploy && npm run version:minor && npm publish",
    "deploy:major": "npm run predeploy && npm run version:major && npm publish",
    "publish:dry-run": "npm publish --dry-run"
  },
  "dependencies": {
    "@rozenite/plugin-bridge": "^1.0.0-alpha.15",
    "nanoevents": "^9.1.0"
  },
  "devDependencies": {
    "@rozenite/vite-plugin": "workspace:*",
    "postcss": "^8.5.6",
    "typescript": "^5.7.3",
    "@types/react": "~18.3.12",
    "@types/react-dom": "~18.3.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "license": "MIT"
}
